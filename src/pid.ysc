      </vertices>
      <vertices xsi:type="sgraph:State" xmi:id="_tKSG8JfLEe2dTL6qMzanMg" specification="entry /&#xD;&#xA;&#x9;pid.known_state = 1.0;&#xD;&#xA;&#x9;pid.proportionalGain = conf.proportionalGain;&#xD;&#xA;&#x9;pid.controlPeriod = conf.controlPeriod;&#xD;&#xA;&#x9;pid.integralTime = conf.integralTime;&#xD;&#xA;&#x9;pid.derivativeTime = conf.derivativeTime;&#xD;&#xA;&#x9;pid.trackingTime = conf.trackingTime;&#xD;&#xA;&#x9;pid.minOutput = conf.minOutput;&#xD;&#xA;&#x9;pid.maxOutput = conf.maxOutput;&#xD;&#xA;&#x9;pid.u0 = conf.minOutput&#xD;&#xA;&#x9;" name="check_config" incomingTransitions="_tKRf5ZfLEe2dTL6qMzanMg">
