      <vertices xsi:type="sgraph:State" xmi:id="_Pci5wJjeEe2G65AmigAwVw" specification="entry / &#xD;&#xA;&#x9;pid.known_state = 4.0;&#xD;&#xA;&#x9;pid.pidoperation = 1&#xD;&#xA;&#xD;&#xA;exit /&#xD;&#xA;&#x9;output = 0.0;&#xD;&#xA;&#x9;pid.pidoperation = 0;&#xD;&#xA;&#x9;pid.errorValue = 0; &#x9;&#xD;&#xA;&#x9;pid.errorminus1 = 0;&#xD;&#xA;&#x9;pid.P = 0;&#xD;&#xA;&#x9;pid.I = 0;&#xD;&#xA;&#x9;pid.D = 0;&#xD;&#xA;&#x9;pid.u0Percentage = 0;&#xD;&#xA;&#x9;pid.controlValue = 0;&#xD;&#xA;&#x9;pid.buffer_output = 0" name="operation" incomingTransitions="_YVq2kJjeEe2G65AmigAwVw">
        <outgoingTransitions xmi:id="_lXhiEJv5Ee24ac6sybmASg" specification="[target == 0.0]" target="_N3GhcJfXEe2dTL6qMzanMg"/>
