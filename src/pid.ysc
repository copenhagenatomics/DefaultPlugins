        <regions xmi:id="_U8U8MJjeEe2G65AmigAwVw" name="operationregion">
          <vertices xsi:type="sgraph:Entry" xmi:id="_U8U8MZjeEe2G65AmigAwVw">
            <outgoingTransitions xmi:id="_U8U8MpjeEe2G65AmigAwVw" specification="" target="_U8U8M5jeEe2G65AmigAwVw"/>
          </vertices>
          <vertices xsi:type="sgraph:State" xmi:id="_U8U8M5jeEe2G65AmigAwVw" specification="entry / &#xD;&#xA;&#x9;pid.known_state = 4.1;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//we calculate all on every control period to support live updates&#xD;&#xA;&#x9;pid.controlPeriod = pid.controlPeriod >= 0.1 ? pid.controlPeriod : 0.1; //minimum control period supported is 0.1&#xD;&#xA;&#x9;pid.integralTime = pid.integralTime >= 0.1 ? pid.integralTime : 0.1; //minimum integral time supported is 0.1&#xD;&#xA;&#x9;pid.trackingTime = pid.trackingTime >= 0.1 ? pid.trackingTime : 0.1; //minimum tracking time supported is 0.1&#xD;&#xA;&#x9;pid.u0Percentage = ((pid.u0 - pid.minOutput) / (pid.maxOutput - pid.minOutput)) * 100.0;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//the PID&#xD;&#xA;&#x9;pid.errorminus1 = pid.errorValue;&#xD;&#xA;&#x9;pid.errorValue = target - input;&#xD;&#xA;&#x9;pid.P = pid.proportionalGain * pid.errorValue;&#xD;&#xA;&#x9;pid.I += (pid.proportionalGain * pid.controlPeriod / pid.integralTime) * pid.errorValue;&#xD;&#xA;&#x9;pid.D = ((pid.proportionalGain * pid.derivativeTime) / pid.controlPeriod) *  (pid.errorValue - pid.errorminus1);&#xD;&#xA;&#x9;pid.controlValue = pid.u0Percentage + pid.P + pid.I + pid.D;&#xD;&#xA;&#xD;&#xA;&#x9;//integral back-calculation/tracking&#xD;&#xA;&#x9;//we adjust the integral to get the control value in the 0-100% power range, &#xD;&#xA;&#x9;//but we don't do it at once but we do it in smaller steps over pid.trackingTime;&#xD;&#xA;&#x9;//so the control value can temporarily be outside the 0-100% range, while the actual output is still adjusted to be in the min - max output range bellow&#xD;&#xA;&#x9;pid.I = pid.controlValue > 100.0 ? pid.I - ((pid.controlValue - 100.0) * pid.controlPeriod / pid.trackingTime) : pid.I;&#xD;&#xA;&#x9;pid.I = pid.controlValue &lt; 0.0 ? pid.I - (pid.controlValue * pid.controlPeriod / pid.trackingTime) : pid.I;&#xD;&#xA;&#x9;pid.controlValue = pid.u0Percentage + pid.P + pid.I + pid.D;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//convert to minOutput-maxOutput range&#xD;&#xA;&#x9;pid.buffer_output = pid.minOutput + (pid.maxOutput - pid.minOutput) * pid.controlValue / 100.0;&#xD;&#xA;&#x9;pid.buffer_output = pid.buffer_output > pid.maxOutput ? pid.maxOutput : pid.buffer_output;&#xD;&#xA;&#x9;pid.buffer_output = pid.buffer_output &lt; pid.minOutput ? pid.minOutput : pid.buffer_output; &#xD;&#xA;&#x9;output = pid.buffer_output&#xD;&#xA;&#x9;&#xD;&#xA;vector / &#xD;&#xA;&#x9;output = pid.buffer_output" name="calculate_values" incomingTransitions="_U8U8MpjeEe2G65AmigAwVw _jfmJoJjkEe2G65AmigAwVw">
            <outgoingTransitions xmi:id="_tVOeEJj8Ee25m-KWkyRGeg" specification="after conf.controlPeriod s" target="_jHUpAJjjEe2G65AmigAwVw"/>
          </vertices>
